{
  "name": "trainer-student-app",
  "version": "1.0.0",
  "description": "Unified Trainer and Student App for MAF",
  "main": "index.js",
  "scripts": {
    "install-all": "cd apps/client && npm install && cd ../server && npm install",
    "start-server": "cd apps/server && npm start",
    "start-client": "cd apps/client && npm start",
    "dev": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "build": "cd apps/client && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "express",
    "trainer",
    "student",
    "martial-arts"
  ],
  "author": "MAF Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "react-router-dom": "^7.7.0"
  }
}
